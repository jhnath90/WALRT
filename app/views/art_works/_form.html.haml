%div.center_div
  = form_for @art_work, html: { multipart: true } do |f|
    = f.label 'Name'
    = f.text_field :name, class: 'form-control'
    %br
    = f.label 'Image'
    = f.file_field :image, class: 'btn'
    %br
    = f.label 'Artist'
    = f.text_field :artist, class: 'form-control'
    %br
    = f.submit class: 'btn'
